<!DOCTYPE html>
<html lang="en">

<head>
    <% include ./partials/head %>
</head>

<body>
    <% include ./partials/header %>
    <section class="section">
        <div class="container">
        <h1 class="subtitle">Problems</h1>
            <table class="table is-hoverable is-fullwidth">
                <thead>
                  <tr>
                    <th>Unit Cost</th>
                    <th>Setup Cost</th>
                    <th>Holding Cost</th>
                    <th>N of Periods</th>
                    <th>Inventory Initial</th>
                    <th>Demands</th>
                    <th>Link</th>
                  </tr>
                </thead>
                <tbody>
                <% results.forEach(result => { %>
                    <tr>
                        <th><%= result.dat.p %></th>
                        <th><%= result.dat.q %></th>
                        <th><%= result.dat.hcost %></th>
                        <th><%= result.dat.NT %></th>
                        <th><%= result.dat.sInit %></th>
                        <th><%= result.djString %></th>
                        <th><a href="/results/<%= result._id %>">Details</a></th>
                    </tr>
                <% }) %>
                </tbody>
              </table>
            </div>
          </section>

    <script src="/javascripts/burguer.js"></script>
</body>

</html>